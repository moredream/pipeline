
<h3 id="" class="page-header">Jobs in Labs</h3>
<%= link_to 'New lab', new_lab_path ,:class => 'btn btn-default' %>
<h3></h3>
<div  id="lab" class="row">
<% labs.each do |lab| %>
  <div class="col-xs-12  col-sm-12 col-md-12">
      <div class="thumbnail docs-container">
        <%= render partial: "lab_item", locals: { title: lab.name, item: lab, content: '' } %>
      </div>
  </div>

<% end %>
</div>

