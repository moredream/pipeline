<h1>Listing Members</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.username %></td>
        <td><%= member.email %></td>
        <td><%= link_to 'Show', member_path(member.id)%></td>
      </tr>
    <% end %>
  </tbody>
</table>
